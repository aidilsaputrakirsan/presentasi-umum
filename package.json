{
  "name": "presentasi-umum",
  "version": "1.0.0",
  "description": "Dashboard sentral presentasi Slidev",
  "main": "server.js",
  "scripts": {
    "predev": "node generate-list.js",
    "dev": "node server.js",
    "build:komputasiawan": "slidev build Komputasi-Awan/slides.md --base /presentasi-umum/Komputasi-Awan/ --out ../dist/Komputasi-Awan",
    "build:strukturdata": "slidev build Struktur-Data/slides.md --base /presentasi-umum/Struktur-Data/ --out ../dist/Struktur-Data",
    "build:cpns": "slidev build CPNS/slides.md --base /presentasi-umum/CPNS/ --out ../dist/CPNS"
  },
  "dependencies": {
    "@slidev/cli": "^0.49.0",
    "@slidev/theme-default": "^0.25.0",
    "@slidev/theme-seriph": "^0.25.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "playwright-chromium": "^1.58.2"
  }
}
